class Solution:
    def grayCode(self, n: int) -> List[int]:
        if n==0:
            return 0
        a=[]
        #def graycode(n):
        #elif n==1:
            #return [0,1]
        for i in range(1<<n):
                #a=bin(i^(i>>1))
            a.append(i^(i>>1))
            #print(a)
                
        return a
        #graycode(n)
